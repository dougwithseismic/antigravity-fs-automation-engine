{
  "name": "antigravity",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:system": "concurrently -n \"API,Worker,Demo\" -c \"blue,green,yellow\" \"pnpm --filter api dev\" \"pnpm --filter @antigravity/worker dev\" \"pnpm --filter demo-react dev\"",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "storybook": "pnpm --filter @repo/ui storybook",
    "db:reset": "pnpm --filter @repo/database db:reset",
    "db:seed": "pnpm --filter @repo/database db:seed",
    "antigravity:start": "pnpm --filter @antigravity/vscode-interceptor start",
    "antigravity:stop": "pnpm --filter @antigravity/vscode-interceptor stop",
    "patch-vscode": "pnpm --filter @antigravity/vscode-interceptor patch-vscode",
    "unpatch-vscode": "pnpm --filter @antigravity/vscode-interceptor unpatch-vscode",
    "view-logs": "pnpm --filter @antigravity/vscode-interceptor view-logs"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "supabase": "^2.58.5",
    "tsx": "^4.0.0",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.2.0",
      "react-dom": "^19.2.0",
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2",
      "@headlessui/react": "^2.0.0"
    }
  },
  "dependencies": {
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}